{
	"name": "doekenorg/gf-entries-in-excel",
	"description": "Never waste time importing CSV's again.",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Doeke Norg",
			"email": "doekenorg@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:GFExcel/base.git"
		}
	],
	"require": {
		"ext-json": "*",
		"php": "^7.3",
		"gfexcel/base": "dev-master",
		"composer/installers": "~1.0",
		"frontpack/composer-assets-plugin": "^0.11.0"
	},
	"autoload": {
		"psr-4": {
			"GFExcel\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"GFExcel\\Tests\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "7.5.*",
		"10up/wp_mock": "0.4.*",
		"szepeviktor/phpstan-wordpress": "^0.6.2",
		"wp-premium/gravityforms": "^2.4",
		"phpstan/phpstan-mockery": "^0.12.5",
		"overtrue/phplint": "^2.1"
	},
	"scripts": {
		"linter": "vendor/bin/phplint --no-cache",
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"assets-dir": "public/addon",
		"assets-files": {
			"gfexcel/base": "public"
		}
	}
}
