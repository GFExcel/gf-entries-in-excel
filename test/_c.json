{
  "autoload": {
    "classmap": [
      "../vendor_prefixed/"
    ],
    "psr-4": {
      "GFExcel\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.*",
    "10up/wp_mock": "0.4.*",
    "szepeviktor/phpstan-wordpress": "^0.6.2",
    "phpstan/phpstan-mockery": "^0.12.5",
    "overtrue/phplint": "^2.1"
  },
  "scripts": {
    "linter": "vendor/bin/phplint --no-cache",
    "test": "vendor/bin/phpunit",
    "prefix_vendor": "@php vendor/gravitykit/gravityexport-lite/strauss.phar",
    "post-install-cmd": [
      "@prefix_vendor",
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "@prefix_vendor",
      "composer dump-autoload -o"
    ]
  },
  "extra": {
    "strauss": {
      "target_directory": "vendor_prefixed",
      "namespace_prefix": "GFExcel\\Vendor",
      "classmap_prefix": "GFExcel_Vendor",
      "classmap_output": true,
      "delete_vendor_packages": true,
      "exclude_from_prefix": {
        "namespaces": [
          "GFExcel\\"
        ]
      },
      "override_autoload": {
        "gravitykit/gravityexport-base": {
          "files": [
            "templates",
            "public",
            "src"
          ]
        }
      }
    }
  }
}
